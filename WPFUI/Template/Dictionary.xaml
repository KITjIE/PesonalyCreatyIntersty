    <converter:GetLength x:Key="getLength"/>
    <!--TextBoxInputGroupA-->
    <Style x:Key="TextBoxInputGroupA" TargetType="{x:Type TextBox}">
        <Setter Property="Background" Value="{DynamicResource {x:Static SystemColors.WindowBrushKey}}"/>
